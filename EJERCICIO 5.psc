Algoritmo TABLAS_DE_MULTIPLICAR
	definir num,factor, mult como entero
	Escribir "escriba un numero"
	Leer num
	Escribir "ingrese el  numero de un factor"
	Leer factor
	c=0
	Mientras c<= factor Hacer
		mult=num*c
		Escribir num "x" c "=", mult
		c=c+1
	FinMientras
	
	
	

	
FinAlgoritmo
